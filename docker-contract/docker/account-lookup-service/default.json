          "prefix": "moja_",
          "defaultLabels": {
            "serviceName": "account-lookup-service"
          }
        }
      }
    },
    "ENDPOINT_SECURITY":{
      "JWS": {
        "JWS_SIGN": false,
        "FSPIOP_SOURCE_TO_SIGN": "switch",
        "JWS_SIGNING_KEY_PATH": "secrets/jwsSigningKey.key"
      }
