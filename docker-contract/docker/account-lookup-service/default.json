      "PORT": 3306,
      "USER": "account_lookup",
      "PASSWORD": "password",
      "DATABASE": "account_lookup",
      "POOL_MIN_SIZE": 10,
      "POOL_MAX_SIZE": 10,
      "ACQUIRE_TIMEOUT_MILLIS": 30000,
