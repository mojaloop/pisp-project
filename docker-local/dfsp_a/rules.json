[
  {
    "ruleId": 1,
    "description": "Returns an Payee rejected transaction error (ML error 5105) from the simulator when transfer value is 5105 in any currency",
    "conditions": {
      "all": [
        {
          "fact": "path",
          "operator": "equal",
          "value": "/transfers"
        },
        {
          "fact": "method",
          "operator": "equal",
          "value": "POST"
        },
        {
          "fact": "body",
          "operator": "equal",
          "value": "5105",
          "path": "$.amount"
        }
      ]
    },
    "event": {
      "type": "simulateError",
      "params": {
        "statusCode": 500,
        "body": {
          "statusCode": "5105",
          "message": "Payee rejected transaction"
        }
      }
    }
  },
  {
    "ruleId": 2,
    "description": "makes the validation of authorization",
    "conditions": {
      "all": [
        {
          "fact": "path",
          "operator": "equal",
          "value": "/verify-authorization"
        },
        {
          "fact": "method",
          "operator": "equal",
          "value": "POST"
        }
      ]
    },
    "event": {
      "type": "FIXED_CALLBACK",
      "params": {
        "statusCode": 200,
        "body": {
          "isValid": true
        }
      }
    }
  },
